@model IEnumerable<eCommerce.Core.Product>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in @Model)
{
    
    <div>

        <div>
            Product Id : @item.ProductId
        </div>

        <div>
            Product Name : @item.ProductName
        </div>

        <div>
            Price : @item.UnitPrice
        </div>
    </div>
    
    
}
